{
    "name": "Semantic MediaWiki Graph",
    "author": [
		"Tobias Weller"
    ],
    "license-name": "Apache License 2.0",
    "url": "https://www.mediawiki.org/wiki/Extension:Semantic_MediaWiki_Graph",
    "description": "Visualizes the semantic information of any Semantic MediaWiki page",
    "type": "parserhook",
    "requires": {
		"MediaWiki": ">= 1.38.0",
		"extensions": {
			"SemanticMediaWiki": "*"
		}
	},
	"ResourceModules": {
		"ext.SemanticMediaWikiGraph": {
			"localBasePath": "modules/ext.SemanticMediaWikiGraph",
    		"remoteExtPath": "SemanticMediaWikiGraph/modules/ext.SemanticMediaWikiGraph",
        	"packageFiles": [
				"app.js",
				"d3_v4.min.js",
				"select2.full.min.js",
				"utility.js"
			],
	        "dependencies": [
				"jquery.ui",
				"mediawiki.util"
			]
    	}
    },
	"manifest_version": 2
}
